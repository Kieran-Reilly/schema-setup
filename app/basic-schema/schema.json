{
  "variables": {
    "translations": {
      "welcome": "Welcome",
      "username": "User Name"
    },
    "data": {
      "username": "Kieran Reilly"
    }
  },
  "datasets": [
    {
      "id": 0,
      "name": "model",
      "fields": [
        {
          "name": "username"
        }
      ]
    }
  ],
  "body": {
    "elements": [
      {
        "element": "h2",
        "content": "Basic Form"
      },
      {
        "element": "h3",
        "content": "@translations.welcome"
      },
      {
        "element": "label",
        "content": "@translations.username"
      },
      {
        "element": "input",
        "field": "model.username",
        "attributes": {
          "aria-label.bind": "@translations.username"
        }
      },
      {
        "element": "p",
        "content": "${model.username}"
      }
    ]
  }
}